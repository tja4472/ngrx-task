{
  "name": "ng-task",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tja4472/ngrx-task"
  },
  "scripts": {
    "--- emulator ---": "",
    "emulator-demo-1": "firebase emulators:start --only firestore,auth --project demo-1",
    "emulator-exec-test": "firebase emulators:exec --only firestore,auth --project demo-1 'npm test -- --forceExit'",
    "emulator-export": "firebase emulators:export emulator-data  --project demo-1",
    "emulator-import": "firebase emulators:start --only firestore,auth --project demo-1 --import emulator-data",
    "--- cypress ---": "",
    "cypress:open:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:run:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:run",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run --e2e --browser chrome --config video=false",
    "--- ---": "",
    "ng": "ng",
    "start": "ng serve",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "deploy": "ng deploy --base-href /ngrx-task/",
    "test": "jest",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier:write": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/cdk": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/fire": "7.4.1",
    "@angular/forms": "14.1.0",
    "@angular/material": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "@angular/service-worker": "14.1.0",
    "@ngrx/component-store": "14.0.2",
    "@ngrx/effects": "14.0.2",
    "@ngrx/entity": "14.0.2",
    "@ngrx/router-store": "14.0.2",
    "@ngrx/schematics": "14.0.2",
    "@ngrx/store": "14.0.2",
    "@ngrx/store-devtools": "14.0.2",
    "angular-cli-ghpages": "0.6.2",
    "date-fns": "2.29.1",
    "firebase": "9.9.1",
    "ngrx-etc": "7.0.1",
    "rxfire": "6.0.3",
    "rxjs": "7.5.4",
    "ts-is-present": "1.2.2",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@angular/language-service": "14.1.0",
    "@ngneat/spectator": "10.0.0",
    "@testing-library/angular": "12.1.1",
    "@testing-library/user-event": "12.1.6",
    "@types/jest": "^28.1.6",
    "@types/node": "12.12.26",
    "codelyzer": "^6.0.0",
    "cross-env": "7.0.3",
    "cypress": "10.3.1",
    "cypress-firebase": "2.2.2",
    "firebase-admin": "11.0.1",
    "firebase-tools": "11.4.1",
    "first-input-delay": "0.1.3",
    "husky": "4.3.0",
    "jasmine-core": "4.0.0",
    "jasmine-marbles": "0.9.1",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "28.1.3",
    "jest-preset-angular": "12.2.0",
    "karma": "6.3.15",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "2.2.5",
    "lint-staged": "10.4.0",
    "prettier": "2.7.1",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write"
    ]
  }
}
