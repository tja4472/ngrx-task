{
  "name": "ng-task",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tja4472/ngrx-task"
  },
  "scripts": {
    "--- webserver ---": "",
    "webserver": "npx http-server -p 8070 -c-1 dist/ng-task",
    "--- emulator ---": "",
    "emulator-demo-1": "firebase emulators:start --only firestore,auth --project demo-1",
    "emulator-exec-test": "firebase emulators:exec --only firestore,auth --project demo-1 'npm test -- --forceExit'",
    "emulator-export": "firebase emulators:export emulator-data  --project demo-1",
    "emulator-import": "firebase emulators:start --only firestore,auth --project demo-1 --import emulator-data",
    "--- cypress:emulate-demo ---": "",
    "cypress:open:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:run:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:run",
    "--- cypress ---": "",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open --e2e",
    "cypress:open:component": "cypress open --component",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run --e2e --browser chrome --config video=false",
    "--- ---": "",
    "cypress:run:component": "cypress run --component --browser chrome --config video=false",
    "ng": "ng",
    "start": "ng serve",
    "start:development": "ng serve --configuration=development",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "deploy": "ng deploy --base-href /ngrx-task/",
    "test": "jest",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/cdk": "14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/fire": "7.4.1",
    "@angular/forms": "14.2.0",
    "@angular/material": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/router": "14.2.0",
    "@angular/service-worker": "14.2.0",
    "@ngrx/component-store": "14.3.0",
    "@ngrx/effects": "14.3.0",
    "@ngrx/entity": "14.3.0",
    "@ngrx/router-store": "14.3.0",
    "@ngrx/store": "14.3.0",
    "@ngrx/store-devtools": "14.3.0",
    "date-fns": "2.29.1",
    "firebase": "9.9.4",
    "rxfire": "6.0.3",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.1",
    "@angular-eslint/builder": "14.0.3",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "14.2.1",
    "@angular/compiler-cli": "14.2.0",
    "@angular/language-service": "14.2.0",
    "@ngneat/spectator": "11.1.3",
    "@ngrx/eslint-plugin": "14.3.0",
    "@ngrx/schematics": "14.3.0",
    "@testing-library/angular": "12.1.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.6",
    "@types/node": "16.11.47",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "angular-cli-ghpages": "1.0.3",
    "cross-env": "7.0.3",
    "cypress": "10.7.0",
    "cypress-firebase": "2.2.2",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-testing-library": "5.6.0",
    "firebase-admin": "11.0.1",
    "firebase-tools": "11.8.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-preset-angular": "12.2.2",
    "ng-mocks": "14.2.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
