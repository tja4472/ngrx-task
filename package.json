{
  "name": "ng-task",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tja4472/ngrx-task"
  },
  "scripts": {
    "--- webserver ---": "",
    "webserver": "npx http-server -p 8070 -c-1 dist/ng-task",
    "--- emulator ---": "",
    "emulator-demo-1": "firebase emulators:start --only firestore,auth --project demo-1",
    "emulator-exec-test": "firebase emulators:exec --only firestore,auth --project demo-1 'npm test -- --forceExit'",
    "emulator-export": "firebase emulators:export emulator-data  --project demo-1",
    "emulator-import": "firebase emulators:start --only firestore,auth --project demo-1 --import emulator-data",
    "--- cypress:emulate-demo ---": "",
    "cypress:open:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:open",
    "cypress:run:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" npm run cypress:run",
    "--- cypress ---": "",
    "cypress:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" GCLOUD_PROJECT=\"emulators-codelab-a5a89\" npm run cypress:open",
    "cypress:open": "cypress open --e2e",
    "cypress:open:component": "cypress open --component",
    "cypress:open-real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"serviceAccount-real.json\" npm run cypress:open",
    "cypress:run": "cypress run --e2e --browser chrome --config video=false",
    "--- ---": "",
    "cypress:run:component": "cypress run --component --browser chrome --config video=false",
    "ng": "ng",
    "start": "ng serve",
    "start:development": "ng serve --configuration=development",
    "start:emulator-demo": "ng serve --configuration=emulator-demo",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "deploy": "ng deploy --base-href /ngrx-task/",
    "test": "jest",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.5",
    "@angular/cdk": "16.0.4",
    "@angular/common": "16.0.5",
    "@angular/compiler": "16.0.5",
    "@angular/core": "16.0.5",
    "@angular/fire": "7.6.1",
    "@angular/forms": "16.0.5",
    "@angular/material": "16.0.4",
    "@angular/platform-browser": "16.0.5",
    "@angular/platform-browser-dynamic": "16.0.5",
    "@angular/router": "16.0.5",
    "@angular/service-worker": "16.0.5",
    "@ngrx/component-store": "16.0.1",
    "@ngrx/effects": "16.0.1",
    "@ngrx/entity": "16.0.1",
    "@ngrx/router-store": "16.0.1",
    "@ngrx/store": "16.0.1",
    "@ngrx/store-devtools": "16.0.1",
    "core-js": "3.30.2",
    "date-fns": "2.30.0",
    "firebase": "9.22.2",
    "node-fetch": "2.6.11",
    "rxfire": "6.0.3",
    "rxjs": "7.8.1",
    "tslib": "2.5.3",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.5",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.0.5",
    "@angular/compiler-cli": "16.0.5",
    "@angular/language-service": "16.0.5",
    "@ngrx/eslint-plugin": "16.0.1",
    "@ngrx/schematics": "16.0.1",
    "@testing-library/angular": "14.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.16",
    "@types/node-fetch": "2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "angular-cli-ghpages": "1.0.6",
    "cross-env": "7.0.3",
    "cypress": "12.14.0",
    "cypress-firebase": "2.2.5",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-testing-library": "5.11.0",
    "firebase-admin": "11.9.0",
    "firebase-tools": "12.3.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "ng-mocks": "14.10.1",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  }  
}
